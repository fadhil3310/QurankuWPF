												<SolidColorBrush x:Key="FluentBorderColor" Color="#19000000" />

												<SolidColorBrush x:Key="FluentTrackColor" Color="Gray" />

												<SolidColorBrush x:Key="FluentContextMenuBackground" Color="#fafafa"/>
