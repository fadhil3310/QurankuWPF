<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

				<ResourceDictionary.MergedDictionaries>
								<ResourceDictionary>
												<Color x:Key="AccentColor">#68c760</Color>
												<SolidColorBrush x:Key="AccentColorBrush" Color="{DynamicResource AccentColor}" />
												<SolidColorBrush x:Key="FluentForegroundColor" Color="Black" />
												<SolidColorBrush x:Key="FluentWindowColor" Color="#FFF9F9F9" />
												<SolidColorBrush x:Key="FluentBackgroundColor" Color="White" />
												<SolidColorBrush x:Key="FluentFrameColor" Color="#fcfcfc" />
